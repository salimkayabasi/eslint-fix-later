{
  "name": "eslint-fix-later",
  "version": "1.0.0",
  "description": "Fix your eslint errors later",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salimkayabasi/eslint-fix-later.git"
  },
  "license": "MIT",
  "author": {
    "name": "Salim KAYABASI",
    "email": "salim.kayabasi@gmail.com"
  },
  "bin": {
    "eslint-fix-later": "scripts/cli.js"
  },
  "main": "index.js",
  "files": [
    "src/**/*",
    "scripts/**/*",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "lint:fix:later": "npx eslint-fix-later .",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "log4js": "^6.4.7"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.6.2"
  }
}
