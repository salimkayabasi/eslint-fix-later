{
  "name": "eslint-fix-later",
  "version": "1.0.0",
  "description": "Fix your eslint errors later",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salimkayabasi/eslint-fix-later.git"
  },
  "license": "MIT",
  "author": {
    "name": "Salim KAYABASI",
    "email": "salim.kayabasi@gmail.com"
  },
  "bin": {
    "eslint-fix-later": "scripts/cli.js"
  },
  "main": "index.js",
  "files": [
    "src/**/*",
    "scripts/**/*",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "log4js": "^6.3.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "prettier": "^2.4.1"
  }
}
